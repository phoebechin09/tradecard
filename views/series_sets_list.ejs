<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= seriesList.name %> Series</title>
    <link href="https://css.gg/css" rel="stylesheet" />
    <script src="/js/uikit.min.js"></script>
    <link rel="stylesheet" href="/css/uikit.min.css">
    <script src="/js/uikit-icons.js"></script>
</head>
<body>
    

    
    <div class="uk-margin uk-margin-large-left">
       <h6>Showing <%= seriesList.sets.length %> Set(s) from <%= seriesList.name %> :</h6> 
    </div>

    <div class="uk-grid-small uk-margin-large-left uk-margin-large-right uk-margin-medium-bottom uk-child-width-1-3@s uk-child-width-1-4@m uk-flex-center"
            uk-grid uk-height-match="target: .uk-card">
            <% seriesList.sets.forEach(set=> { %>
                <div>
                    <div class="uk-card uk-card-default uk-card-hover ">
                        <div class="uk-margin-small-left uk-padding-small">
                            <a class="uk-link-reset" href="#">
                                <%= set.name %>
                            </a>
                        </div>
                        <div class="uk-card-media-bottom uk-padding-small ">

                            <% if (set.logo){ %>
                                <a href="#"><img src="<%= set.logo %>.png" alt=""></a>
                                <% } else { %>
                                    <div class="uk-text-center">
                                        <a class="uk-link-reset uk-text-small uk-text-muted " href="#">no image üñºÔ∏è</a>
                                    </div>

                                    <% } %>

                        </div>
                    </div>
                </div>
                <% }); %>
        </div>
    

    <%- include('footer') %>
</body>
</html>